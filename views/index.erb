<script src="//ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js" ></script>
<script type="text/javascript">
  var callback = function(){
    $.getJSON('/canvas_redirect', '', function(data){
      debugger;
      if(data.canvas_redirect){
        location.href = data.canvas_redirect;
      } else {
        window.setTimeout(callback, 3000);
      }
    });
  }

  window.setTimeout(callback, 1000);
</script>

<body style="margin:0px">
  <iframe src="<%= iframe_src %>" style="margin: 0px; padding: 0px" width="100%" height="100%" frameborder="0"></iframe>
</body>